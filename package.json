{
  "name": "pubsub-client",
  "displayName": "pubsub-client",
  "description": "Interactive client to send/receive pubsub messages",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pubsubClient",
          "title": "PubSub Client",
          "icon": "/media/pubsub.svg"
        }
      ]
    },
    "views": {
      "pubsubClient": [
        {
          "id": "pubsubClient.subscriptions",
          "name": "Subscriptions",
          "icon": "media/pubsub.svg",
          "contextualTitle": "Subscriptions"
        }
      ]
    },
    "commands": [
      {
        "command": "pubsubClient.addSubscription",
        "title": "Add",
        "icon": "$(add)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "pubsubClient.addSubscription",
          "group": "navigation",
          "when": "view == pubsubClient.subscriptions"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "pubsubClient.subscriptions",
        "contents": "No pubsub subscriptions found.\n[Add Subscription](command:pubsubClient.addSubscription)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.0",
    "@vscode-elements/elements": "^2.3.1"
  }
}
